{
    "key": "DEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMODEMO",
    "name": "cookie-store",
    "version": "1.0.0",
    "description": "chrome extension for cookie crud on a per site basis with user login and encrypted storage",
    "icons": {
        "16": "/app/img/icon_16x16.png",
        "48": "/app/img/icon_48x48.png",
        "128": "/app/img/icon_128x128.png"
    },
    "homepage_url": "https://github.com/angeal185/cookie-store",
    "browser_action": {
        "default_icon": {
            "19": "/app/img/icon_19x19.png",
            "38": "/app/img/icon_38x38.png"
        },
        "default_title": "cookie-store",
        "default_popup": "index.html"
    },
    "devtools_page": "/devtools/devtools-page.html",
    "background": {
        "scripts": [
            "/app/js/vendor.js",
            "/app/js/utils.js",
            "/app/js/data.js",
            "/app/js/background.js",
            "/devtools/background-devtools.js"
        ]
    },
    "options_page": "options.html",
    "permissions": [
        "tabs",
        "\u003Call_urls\u003E",
        "cookies",
        "contextMenus",
        "unlimitedStorage",
        "notifications",
        "storage",
        "privacy",
        "clipboardWrite",
        "clipboardRead",
        "system.cpu"
    ],
    "content_security_policy": "script-src 'unsafe-eval' 'self';  object-src 'self'",
    "minimum_chrome_version": "41",
    "manifest_version": 2,
    "update_url": "http://clients2.google.com/service/update2/crx"
}
